{
    "name": "mlambley/swagvalidator",
    "description": "Validates data against a Swagger schema",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Mark Lambley",
            "email": "mlambley@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Mlambley\\Swagvalidator\\": "src/"
        }
    },
    "require": {
        "php": ">=5.6"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.12",
        "codeception/codeception": "^4.1",
        "codeception/module-phpbrowser": "^1.0.0",
        "codeception/module-asserts": "^1.0.0",
        "codeception/aspect-mock": "^3.1"
    },
    "scripts": {
        "lint": "php-cs-fixer fix --config=.php_cs.dist.php"
    }
}
